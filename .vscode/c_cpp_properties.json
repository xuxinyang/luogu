{
  "configurations": [
    {
      "name": "windows-gcc-x64",
      "includePath": [
        "${workspaceFolder}/**"
      ],
      "defines": [
        "_DEBUG",
        "UNICODE",
        "_UNICODE"
      ],
      "cStandard": "${default}",
      "cppStandard": "${default}",
      "intelliSenseMode": "windows-gcc-x64",
      "compilerArgs": [],
      "compilerPath": "gcc",
      "browse": {
        "path": [
          "${workspaceFolder}/**",
          "C:/mingw64/include**",
          "C:/mingw64/lib/gcc/x86_64-w64-mingw32/8.1.0/include/**"
        ],
        "limitSymbolsToIncludedHeaders": true
      }
    }
  ],
  "version": 4
}